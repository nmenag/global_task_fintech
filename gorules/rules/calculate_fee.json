            "in2": "\"standard\"",
            "out1": "0.02",
            "out2": "\"approved\""
          },
          {
            "in1": "amount <= 5000",
            "out1": "0.05",
            "out2": "\"approved\""
          },
          {
            "out1": "0.1",
            "out2": "\"audit_required\""
          }
        ]
      },
